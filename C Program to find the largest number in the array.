#include<stdio.h>
int main()
{
    int n;
    printf("enter the size of array");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {printf("enter the %dth element of array",i);
    scanf("%d",&a[i]);}
    int greater=a[0];
   for(int i=1;i<n;i++)
  { if(greater>a[i])
   greater=greater;
   else
   greater=a[i];
  }
   printf("%d",greater);
    return 0;
}
